<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="b12app.vyom.com.flowit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/flowit"
        android:label="@string/app_name"
        android:roundIcon="@drawable/flowit"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".home.Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".startupscreen.StartUpScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.ProjectCreateActivity"
            android:theme="@style/ProjectTheme"/>
        <activity android:name=".home.TaskCreateActivity"
            android:theme="@style/TaskTheme"/>
    </application>

</manifest>